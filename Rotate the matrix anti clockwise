#include<iostream>
using namespace std;
#include<vector>
int main()
{
	int n;
	cin>>n;
	int a[n][n];
	int b[n][n];
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			cin>>a[i][j];
		}
	}

  for(int i=0;i<n;i++)
{  int k=n-1;
	for(int j=0;j<n;j++)
	{
		b[k][i]=input[i][j];
		k--;
	}
}
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			input[i][j]=b[i][j];
		}
		
	}
}
