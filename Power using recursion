int power(int x, int n) {
  
    if(n==1)
    {
        return x; 
    }
    else if(n==0)
    {
        return 1;
    }
    int output=power(x,n-1);
    return x*output;

}
#include<iostream>

using namespace std;


int main(){
    int x, n;
    cin >> x >> n;
  
    cout << power(x, n) << endl;
}
